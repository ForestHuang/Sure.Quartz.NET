
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>首页</title>
    <!-- Css -->
    <link type="text/css" href="~/Content/Css/bootstrap.min.css" rel="stylesheet" />
    <link type="text/css" href="~/Content/Css/style.min.css" rel="stylesheet" />
    <link type="text/css" href="~/Content/Css/font-awesome.min.css" rel="stylesheet" />
</head>
<body class="gray-bg">
    <div class="wrapper wrapper-content  animated fadeInRight">
        <div class="row">
            <div class="col-pull-11">
                <div class="ibox">
                    <div class="ibox-content">
                        <span class="text-muted small pull-right">sure</span>
                        <h2>Study Quartz.NET</h2>
                        <div class="clients-list">
                            <ul class="nav nav-tabs" id="nav-jobStatus">
                                <li class="pull-right small text-muted"></li>
                                <li class="active active-jobData" data-jobStatus="-1" v-on:click="loadGetJob()">
                                    <a data-toggle="tab" href="#tab-QuartzData">任务调度管理</a>
                                </li>
                                <li class="">
                                    <a data-toggle="tab" href="#tab-Quartz-Durable">持久化任务调度管理</a>
                                </li>
                            </ul>
                            <div class="tab-content">
                                <!-- Job -->
                                <div id="tab-QuartzData" class="tab-pane active">
                                    <div class="full-height-scroll">
                                        <iframe class="J_iframe" style="width:100%;height:800px;" name="iframe" src="/QuartzManager/QuartzHome?v=4.0" frameborder="0"></iframe>
                                    </div>
                                </div>

                                <!-- Job 持久化，集群 -->
                                <div id="tab-Quartz-Durable" class="tab-pane">
                                    <div class="full-height-scroll">
                                        <iframe class="J_iframe" style="width:100%;height:800px;" name="iframe" src="/QuartzManager/QuartzDurableHome?v=4.0" frameborder="0"></iframe>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- JavaScript -->
    <script type="text/javascript" src="~/Content/Scripts/jquery-1.10.2.min.js"></script>
    <script type="text/javascript" src="~/Content/Scripts/bootstrap.min.js"></script>
</body>

</html>
